<logue>
	<options>
        <option name="ip">192.168.0.104</option>
        <option name="port">34300</option>
        <option name="sideBySide">0</option>
        <option name="monitor">0</option>
        <option name="background">#000000</option>
        <option name="timeout">-1</option>
        <option name="qualityIconPos">icons/feedback/thumb_positive.png</option>
        <option name="qualityIconNeg">icons/feedback/thumb_negative.png</option>
	</options>
	<classes>
        <class name="SpeechRate" event="VoiceActivity" sender="PRAAT">
            <threshold value="0.0" quality="negative" icon="icons/speechrate/orientation_low.png"/>
            <threshold value="2.0" quality="positive" icon="icons/speechrate/orientation_med.png"/>
            <threshold value="4.0" quality="negative" icon="icons/speechrate/orientation_high.png"/>
        </class>
        <class name="BodyEnergy" event="oa" sender="expressivity">
            <threshold value="0.0" quality="negative" icon="icons/energy/area_low.png"/>
            <threshold value="24.0" quality="positive" icon="icons/energy/area_med.png"/>
            <threshold value="70.0" quality="negative" icon="icons/energy/area_high.png"/>
        </class>
        <class name="Openness" event="open" sender="expressivity">
            <threshold value="-999.0" quality="negative" icon="icons/openness/body_low.png"/>
            <threshold value="0.1" quality="positive" icon="icons/openness/body_med2.png"/>
            <threshold value="0.4" quality="positive" icon="icons/openness/body_high.png"/>
        </class>
	</classes>
</logue>
